C
      CHARACTER*8 GWRD,EWRD
C
      DIMENSION TIMSTR(3)
C
      PARAMETER (MXATM=2000, MXRT=100, MXSH=5000, MXAO=8192,
     *           MXFRG=1050, MXNORO=250,
     *           MXDFG=5, MXDPPT=MXFRG*MXDFG*12)
C
