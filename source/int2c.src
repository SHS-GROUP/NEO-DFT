C*MODULE INT2C   *DECK PSPS_BCTE
      SUBROUTINE PSPS_BCTE(WRK1,WRK2,IDIM,JR)
      INTEGER IDIM,I,J,JR
      DOUBLE PRECISION WRK1(*),WRK2(IDIM,*)
      J=       1
      DO I=1,     3
      J=J+1
      WRK2(JR,I)=WRK1(J)
      END DO
      END
