      LOGICAL KSZDOE,KFORB,KPRINT,ORDSKW,DDTFPT
      COMMON /OSGLOBAL/      ENCSUM,THRERI,NELACT,MULT,NSPIN,ISTSYM(3),
     *  NMOFZC,NMODOC,NMOACT,NMOEXT,NMOFZV,NMOCOR,NMO   ,NSOACT,NBF   ,
