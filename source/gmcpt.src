C
      dimension list(4,numprs)
      dimension lor2rf(ndet)
      dimension locc(NSOMAX),lvir(NSOMAX)
      logical   jocc(NSOMAX),phase(NSOMAX)  ! F: + phase, T: - phase
      logical   iphase,kphase,lphase
C
      call os_det2occx(jdeto,jocc,-1)
C
      nocc  =0
      nvir  =0
      jdtsym=0
      ns    =0
      iphase=.false.
C
