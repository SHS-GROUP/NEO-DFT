        lsym=ieor(jdtsym,mosym(ls))
        if(mospn(ls).eq.1) then
          lns=ns-1
        else
          lns=ns+1
        end if
      do k=1,nvir+1
        lr=lvir(k)
        jocc(lr)=.true.
        kphase=lphase.neqv.phase(lr)
        ksym=ieor(lsym,mosym(lr))
        if(mospn(lr).eq.1) then
          kns=lns+1
        else
          kns=lns-1
        end if
      do i=1,k-1
        lp=lvir(i)
        jocc(lp)=.true.
        iphase=kphase.neqv.phase(lp)
        isym=ieor(ksym,mosym(lp))
