      LOGICAL GOPARR,MASWRK,DSKWRK
C
      COMMON /PAR   / ME,MASTER,NPROC,IBTYP,IPTIM,GOPARR,DSKWRK,MASWRK
      COMMON /IOFILE/ IR,IW,IP,IS,IPK,IDAF,NAV,IODA(950)
C
      DOUBLE PRECISION SPOTC(6)
      CHARACTER*110 LINE
      INTEGER IDA,NFTPOT
C
       NFTPOT = 27
       CALL SEQOPN(NFTPOT,'COSPOT','OLD',.TRUE.,'FORMATTED')
      IF (MASWRK) THEN
C
   5  READ(NFTPOT,'(A110)',IOSTAT=KODE) LINE
C
       IF(KODE.EQ.0) THEN
         IDA=INDEX(LINE,'SIGMA POTENTIAL COEFFICIENTS:')
         IF (IDA .LE. 0) GOTO 5
         LINE=LINE(IDA+29:)
         DO 10 I=1,5
          J=INDEX(LINE,'    ')
          LINE=LINE(IDA+J:)
          READ(LINE,'(D12.5)') SPOTC(I)
   10    CONTINUE
         K=INDEX(LINE,'   ')
         LINE=LINE(IDA+K:)
         READ(LINE,'(D9.5)') SPOTC(6)
C PRINTOUT IN OUTPUT FILE
       WRITE(IW,*) '================ COSMO ENABLED ================'
       WRITE(IW,*) 'DCOSMO-RS SWITCHED ON'
       WRITE(IW,*) 'DCOSMO-RS: USING THE FOLLOWING POTENTIAL COEFF.:'
       WRITE(IW,'(D15.5,D15.5,D15.5,D15.5,D15.5,D15.5)') SPOTC(1),
     &    SPOTC(2),SPOTC(3),SPOTC(4),SPOTC(5),SPOTC(6)
       WRITE(IW,*) '==============================================='
C
       ELSE
         WRITE(IW,*) 'NO MORE DATA READ'
       ENDIF
       CALL SEQCLO(NFTPOT,'KEEP')
      ENDIF
      RETURN
      END
