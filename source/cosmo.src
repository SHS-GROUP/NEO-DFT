C*MODULE COSMO  *DECK COSADD
      SUBROUTINE COSADD(BLDCAV)
C***********************************************************************
C
C THIS ROUTINE IS THE MAIN ROUTINE TO ADD COSMO SOLVATION.
C NAMELY IT WILL ADD SOLVATION CONTRIBUTION TO THE 1-ELECTRON INTEGRALS.
C IF CALLED WITH BLDCAV=.TRUE. IT WILL BUILD THE CAVITY (E.G. IN RHFUHF)
C IF BLDCAV=.FALSE. THE CAVITY IS READ FROM NFT26 (E.G. IN MP2)
C
C   CODED BY ROBERTO PEVERATI @ UNIV.OF ZURICH
C
C INPUT:  BLDCAV = LOGICAL
C OUTPUT: SECORR (SOLVATION ENERGY CORRECTIONS, IN COSMO1 COMMON GR.)
C         FEED THE COSMO CHARGES INTO ONEEI
C
************************************************************************
C
      IMPLICIT DOUBLE PRECISION (A-H,O-Z)
C
C
C     COSMO INFORMATION
C
      PARAMETER (MXATM=2000, MAXDEN=25*MXATM,MXABC=6000,NPPA=1082)
      LOGICAL BLDCAV
      LOGICAL EDIELONLY
C
      DOUBLE PRECISION SPOTC(6)
C
      DIMENSION QVPOT(MXABC)
C
      COMMON /CONV  / DENTOL,EN,ETOT,EHF,EHF0,DIFF,ITER,ICALP,ICBET
      COMMON /INFOA / NAT,ICH,MUL,NUM,NQMT,NE,NA,NB,
     *                ZAN(MXATM),C(3,MXATM),IAN(MXATM)
      COMMON /FMCOM / X(1)
      COMMON /OUTPUT/ NPRINT,ITOL,ICUT,NORMF,NORMP,NOPK
      COMMON /PCMPAR/ IPCM,NFT26,NFT27,IKREP,IEF,IP_F,NFMOPCM
C
      LOGICAL ISEPS, USEPS
      COMMON /ISEPS / ISEPS, USEPS
C
      LOGICAL COSBUG,COSWRT,DCOSMO,PRFCND,IOUTCH
      COMMON /COSDAT/ SE2,SECORR,QVCOSMO,ELAST,EMP2COS,EMP2LAST,
     *                COSVOL,COSSAR,EDIEL,EOC1,DEOC_RS,SUMQSC,
     *                SUMQSCOLD,ZSUM,ZSUM2,ZSUM3,FEPSI,RDS,DISEX2,
     *                EPSI,COSRAD,DISEX,OUTCHG,EDIEL_SAVE,
     *                MAXNPS,ICORR,ITRIP,NQS,MP2TRIP,MP2ITER,
     *                ICFREQ,NSPA,NSPH,NPSD,NPS,NPS2,NDEN,NPSPHER,
     *                COSBUG,COSWRT,DCOSMO,PRFCND,IOUTCH
C
      COMMON /COSVCE/ QSCNET(MXABC),CORZAN(3,MXABC),
     *                QDEN(MAXDEN),QSCNET_SAVE(MXABC),IATSP(MXABC+1)
C
      COMMON /IOFILE/ IR,IW,IP,IS,IPK,IDAF,NAV,IODA(950)
      DATA DMULTI/8HDMULTI  /
C
      IF(.NOT. BLDCAV) THEN
        NDEN2=(NDEN*NDEN+NDEN)/2
        LENAB2 = MXABC*(MXABC+5)/2
        CALL VALFM(LOADFM)
        LABCMT = LOADFM + 1
        LAST   = LABCMT + LENAB2
        NEEDC  = LAST - LOADFM - 1
        CALL GETFM(NEEDC)
C RECOVER PREVIOUS A,B,C MATRICES
        NFT26 = 26
        CALL SQREAD(NFT26,X(LABCMT)              ,NPS2)
        LDENPS = NDEN*NPS
        CALL SQREAD(NFT26,X(LABCMT+NPS2)         ,LDENPS)
        CALL SQREAD(NFT26,X(LABCMT+NPS2+NDEN*NPS),NDEN2)
        CALL SEQREW(NFT26)
      ELSE
        ITRIP=1
        IF(OUTCHG.EQ.DMULTI) CALL NUMPROP
C
C FIRST SCF ITERATION NEEDS SURFACE CONSTRUCTION
C ABCMAT HOLDS THREE DIFFERENT MATRICES NAMED A, B, C.
C WHEN A IS FIRST GENERATED, IT IS SQUARE STORAGE.
C
        NDEN2 = (NDEN*NDEN+NDEN)/2
C
C START MEMORY ALLOCATION FOR DC
        LENA = MAXNPS*(MAXNPS+1)/2
        LENB = NDEN*MAXNPS
        LENC = NDEN2
        LENA2= MAXNPS*MAXNPS
        LENAB2 = LENA+LENB+LENC
C
        CALL VALFM(LOADFM)
        LABCMT  = LOADFM  + 1
        LNSET   = LABCMT  + LENAB2
        LDIN    = LNSET   + NPPA*NAT
        LTM     = LDIN    + NPPA + 1
        LDIRTM  = LTM     + 3*3*NAT
        LXSP    = LDIRTM  + 3*NPPA
        LNIPA   = LXSP    + 3*MXABC
        LLIPA   = LNIPA   + MXABC/5
        LSUDE   = LLIPA   + 2*MXABC
        LISUDE  = LSUDE   + MXABC
        LNSETF  = LISUDE  + MXABC
        LNN     = LNSETF  + 2*MAXNPS
        LNFA    = LNN     + 3*NAT
        LA2MAT  = LNFA    + 2*MAXNPS
        LA3MAT  = LA2MAT  + LENA2
        LAMTSAV = LA3MAT  + LENA
        LBH     = LAMTSAV + LENA
        LAST    = LBH     + MXABC
        NEEDC   = LAST - LOADFM - 1
        CALL GETFM(NEEDC)
C
        IF(ICORR.EQ.0) THEN
          NFT26 = 26
          CALL SEQOPN(NFT26,'COSCAV','UNKNOWN',
     *                .FALSE.,'UNFORMATTED')
          CALL COSCAV(X(LABCMT),X(LNSET),X(LDIN),X(LTM),
     *                X(LDIRTM), X(LXSP),X(LNIPA),X(LLIPA),
     *                X(LSUDE),X(LISUDE),X(LNSETF),
     *                X(LNN),X(LNFA),X(LA2MAT),X(LA3MAT),
     *                X(LAMTSAV),LENAB2)
          CALL SQWRIT(NFT26,X(LABCMT),NPS2)
C
C    WRITING A2, A3, AND A COPY OF ABCMAT (FOR OUTCHG=DBLCAV)
C
          CALL DAWRIT(IDAF,IODA,X(LA2MAT),LENA2,501,0)
          CALL DAWRIT(IDAF,IODA,X(LA3MAT),LENA,502,0)
          CALL DAWRIT(IDAF,IODA,X(LAMTSAV),LENA,503,0)
C
        ELSE
          CALL SQREAD(NFT26,X(LABCMT),NPS2)
        END IF
C
C    GENERATE AND SAVE -B- AND -C- MATRICES  (FOR OUTCHG=DMULTI)
C
        CALL BTOC(X(LABCMT),X(LBH),LENAB2)
        CALL SQWRIT(NFT26,X(LABCMT+NPS2)         ,NDEN*NPS)
        CALL SQWRIT(NFT26,X(LABCMT+NPS2+NDEN*NPS),NDEN2)
        CALL SEQREW(NFT26)
      END IF
C
C      CALCULATE SOLUTE POTENTIAL (QVPOT)
C
      DO 34 II=1,MXABC
        QVPOT(II)=0.0D+00
  34  CONTINUE
      CALL COSPOT(QVPOT)
C
C      CALCULATE CHARGES ON SURFACE FROM REAL POTENTIAL (DBLCAV)
C      OR FROM MULTIPOLE POTENTIAL (DMULTI)
C      CALCULATE SOLVATION ENERGY
C
      CALL DECORR(X(LABCMT),LENAB2,QVPOT)
C
      DO I=1,NPS
        QSCNET_SAVE(I)=QSCNET(I)
      ENDDO
C
      IF((ICORR.EQ.0).AND.DCOSMO) THEN
        CALL CSPTREAD(SPOTC)
      ENDIF
C
      IF (DCOSMO) THEN
        EDIEL=SECORR
        EDIELONLY=.FALSE.
        POLSCAL=1.0D+00
        CALL COSMO_RS(SPOTC,EDIEL,EDIELRS,EDIELONLY,
     &                POLSCAL,X(LABCMT),LENAB2)
        EDIEL_SAVE=EDIEL
        SECORR=EDIELRS
      ENDIF
C
      USEPS=.TRUE.
      IF (COSBUG) THEN
        WRITE(IW,*) '==============================================='
        WRITE(IW,*) "NOW WE NEED ENERGY OF THE SOLUTE+SOLVENT:"
      ENDIF
      ICORR=1
      IF (COSBUG) THEN
        WRITE(IW,*)"UPDATING 1 ELECTRON CONTRIBUTION:"
      ENDIF
C
C     FEEDING CHARGES INTO 1 ELECTRON HAMILTONIAN
C        AVOID PRINTING INSIDE THIS ROUTINE...
C
      NPSAVE = NPRINT
      NPRINT = -2183
      CALL ONEEI
      NPRINT = NPSAVE
      IF (COSBUG) THEN
        WRITE(IW,*)"ADDING NUCLEAR/SCREENING CHARGE CONTRIBUTION:"
      ENDIF
C
C     THIS ENERGY IS:
C
      EN=ENUC(NAT,ZAN,C)
      IF (COSBUG) THEN
        WRITE(IW,*) "ETOT, SECORR, EN= ",ETOT,SECORR,EN
        WRITE(IW,*) '==============================================='
      ENDIF
      CALL RETFM(NEEDC)
      RETURN
C
      END
C
C*MODULE COSMO  *DECK COSOCE
      SUBROUTINE COSOCE(ENRTOADD)
C***********************************************************************
C
C THIS ROUTINE CALCULATES THE OUTLYING CHARGE EFFECT (EOC1)
C FOR DMULTI AND DBLCAV, AND ADDS IT TO ENRTOADD.
C EXAMPLE:
C CALL COSOCE(ETOT)
C WILL ADD EOC1 TO ETOT
C
C   CODED BY ROBERTO PEVERATI @ UNIV.OF ZURICH
C
C***********************************************************************
C
      IMPLICIT DOUBLE PRECISION (A-H,O-Z)
C
      PARAMETER (MXATM=2000, MAXDEN=25*MXATM, MXABC=6000)
      PARAMETER (TOANGS=0.52917724924D+00)
C
      LOGICAL EDIELONLY
      DOUBLE PRECISION SPOTC(6)
      DIMENSION QVPOT(MXABC)
      COMMON /FMCOM / X(1)
      COMMON /IOFILE/ IR,IW,IP,IS,IPK,IDAF,NAV,IODA(950)
C
      LOGICAL ISEPS, USEPS
      COMMON /ISEPS / ISEPS, USEPS
C
      LOGICAL COSBUG,COSWRT,DCOSMO,PRFCND,IOUTCH
      COMMON /COSDAT/ SE2,SECORR,QVCOSMO,ELAST,EMP2COS,EMP2LAST,
     *                COSVOL,COSSAR,EDIEL,EOC1,DEOC_RS,SUMQSC,
     *                SUMQSCOLD,ZSUM,ZSUM2,ZSUM3,FEPSI,RDS,DISEX2,
     *                EPSI,COSRAD,DISEX,OUTCHG,EDIEL_SAVE,
     *                MAXNPS,ICORR,ITRIP,NQS,MP2TRIP,MP2ITER,
     *                ICFREQ,NSPA,NSPH,NPSD,NPS,NPS2,NDEN,NPSPHER,
     *                COSBUG,COSWRT,DCOSMO,PRFCND,IOUTCH
C
      COMMON /COSVCE/ QSCNET(MXABC),CORZAN(3,MXABC),
     *                QDEN(MAXDEN),QSCNET_SAVE(MXABC),IATSP(MXABC+1)
C
      DATA DMULTI,DBLCAV/8HDMULTI  ,8HDBLCAV  /
C
C         SOLVATION OUTPUT FOR COSMO MODEL
C
      EOC1=0.0D+00
      IF(OUTCHG.EQ.DBLCAV) THEN
        IOUTCH=.TRUE.
C        EDIELOLD=0.0D+00
C
C    CALCULATE POTENTIAL ON OUTER CAVITY DUE TO SOLUTE AND
C    SCREENING CHARGES ON INNER CAVITY
C
        CALL COSPOT(QVPOT)
C
C    CORRECT POTENTIAL AND CHARGES ON INNER CAVITY
C    CALCULATE CORRECTED SOLVATION ENERGY
C
C FIRST ALLOCATE MEMORY, THEN READ  A2, A3, AND AMTSAV
        NDEN2=(NDEN*NDEN+NDEN)/2
        LENA = MAXNPS*(MAXNPS+1)/2
        LENB = NDEN*MAXNPS
        LENC = NDEN2
        LENA2= MAXNPS*MAXNPS
        LENAB2 = LENA+LENB+LENC
C
        CALL VALFM(LOADFM)
        LA2MAT  = LOADFM  + 1
        LA3MAT  = LA2MAT  + LENA2
        LAMTSAV = LA3MAT  + LENA
        LAST    = LAMTSAV + LENA
        NEEDC   = LAST - LOADFM - 1
        CALL GETFM(NEEDC)
C
          CALL DAREAD(IDAF,IODA,X(LA2MAT),LENA2,501,0)
          CALL DAREAD(IDAF,IODA,X(LA3MAT),LENA,502,0)
          CALL DAREAD(IDAF,IODA,X(LAMTSAV),LENA,503,0)
C
C    CALCULATE OCE
C
        CALL COSMO_OC(QVPOT,DE,X(LA2MAT),X(LA3MAT),X(LAMTSAV))
C
        CALL RETFM(NEEDC)
C
        EOC1=DE
C
        IF (DCOSMO) THEN
          IF(ICORR.EQ.0) THEN
            CALL CSPTREAD(SPOTC)
          ENDIF
          EDIELX=EDIEL_SAVE+DE
          EDIELRS_OLD=SECORR
          EDIELRS=0.0D+00
          EDIELONLY=.TRUE.
          POLSCAL=1.0D+00
C
          NDEN2=(NDEN*NDEN+NDEN)/2
          LENA = NPS*(NPS+1)/2
          LENB = NDEN*NPS
          LENC = NDEN2
          LENAB2 = LENA+NDEN*MAXNPS+NDEN2
C
          CALL VALFM(LOADFM)
          LABCMT = LOADFM + 1
          LAST   = LABCMT + LENAB2
          NEEDC  = LAST - LOADFM - 1
          CALL GETFM(NEEDC)
C
C RECOVER PREVIOUS A,B,C MATRICES
          NFT26 = 26
          CALL SQREAD(NFT26,X(LABCMT)              ,NPS2)
          LDENPS = NDEN*NPS
          CALL SQREAD(NFT26,X(LABCMT+NPS2)         ,LDENPS)
          CALL SQREAD(NFT26,X(LABCMT+NPS2+NDEN*NPS),NDEN2)
          CALL SEQREW(NFT26)
C
          CALL COSMO_RS(SPOTC,EDIELX,EDIELRS,EDIELONLY,
     &                     POLSCAL,X(LABCMT),LENAB2)
          DEOC_RS=EDIELRS-EDIELRS_OLD
          WRITE(IW,*) 'DEOC_RS=',DEOC_RS
        ENDIF
        IOUTCH=.FALSE.
      END IF
C
C   FOR THE DISTRIBUTED MULTIPOLAR OUTLYING CHARGE CORRECTION WE
C   NEED TO RE-EVALUATE THE REAL POTENTIAL ON THE CAVITY.
C   THE CORRECTION IS THE DIFFERENCE BETWEEN THE COSMO POTENTIAL
C   AND THE REAL POTENTIAL.
C   THIS IS DONE HERE ONLY FOR SCF - FOR MP2 THE OUTLYING CHARGE
C   CORRECTION IS SKIPPED AND DONE IN MP2GRD
C
      IF(OUTCHG.EQ.DMULTI) THEN
C
C    CALCULATE QVCORR - USING REAL POTENTIAL FROM GAMESS
C
        QVCORR=0.0D+00
        DO 6 I=1,NQS
C FIX FOR UNIT FOR DMULTI
          QVPOT(I)=QVPOT(I)*TOANGS
          QVCORR = QVCORR + QVPOT(I)*QSCNET(I)
  6     CONTINUE
C
C    CALCULATE "OUTLYING CHARGE" (EOC1): -QV + QV'
C
        EOC1=-QVCORR+QVCOSMO
      END IF
C
C   PEDIFF6 IS NOW THE 'CORRECTED' TOTAL ENERGY AND
C   WE PUT IT BACK INTO ENRTOADD
C
      PEDIFF6=ENRTOADD+EOC1
      ENRTOADD=PEDIFF6
C
C    RESET SOME ESSENTIAL PARAMETERS:
C
      ITRIP=0
      USEPS=.FALSE.
      ELAST=0.0D+00
      RETURN
C
      END
