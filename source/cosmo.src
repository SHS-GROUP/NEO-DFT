C     VERSIONS; NOW INCLUDED AS DATA GROUP; VOLKER NOV 1998
C ERASE THE DIRVEC 1 TO 6 SYSTEME, CALL TO FILL_GRD TO
C FILL THE BASIC GRID
C
C     CHECK FOR CORRECT IMPLEMENTATION
C
      NPPAX = NPPA
      CALL AOLIM()
C
      DO 10 I=1,53
         USEVDW(I)=RVDW(I)
   10 CONTINUE
C
C
C  CORRESPING RADII TO ATOMIC NUMBER
C
      DO 14 J=1,NAT
         IAT=INT(ZAN(J))
         SRAD(J)=USEVDW(IAT)
   14 CONTINUE
C
      FEPSI=(EPSI-1.0D+00)/(EPSI+0.5D+00)
      IF (PRFCND) THEN
        FEPSI=1.0D+00
      ENDIF
C
      IF(MPCTYP.EQ.NONE) THEN
         NDEN=25*NAT
      ELSE
         NDEN=3*NORBS-2*NAT
      ENDIF
C
      NLIT=0
      DO 5 I=1,NAT
   5  IF(INT(ZAN(I)).EQ.1) NLIT=NLIT+1
C
      IF (COSRAD.LT. 0.0D+00) THEN
         WRITE(IW,*) ' COSRAD MUST NOT BE NEGATIVE'
         CALL ABRT
      END IF
C
      FNSPA = NSPA
      X0=LOG(FNSPA*0.1D+00-0.199999D+00)
      Z3=LOG(3.0D+00)
      Z4=LOG(4.0D+00)
      I4=INT(X0/Z4)
      NSPB=0
      DO 7 I=0,I4
         X=X0-I*Z4
         N=3**INT(X/Z3)*4**I
         IF(N.GT.NSPB) NSPB=N
   7  CONTINUE
      NSPH=NSPB/3
      IF(MOD(NSPB,3).NE.0) NSPH=NSPB/4
      NSPA=10*NSPB+2
      NSPH=MAX(12,NSPH*10+2)
      CALL DVFILL(NSPA,DIRSM)
      CALL DVFILL(NSPH,DIRSMH)
C
      MAXNPS = (NAT-NLIT)*NSPA + NLIT*NSPH
      IF(MAXNPS.LE.500) MAXNPS = INT(1.2D+00*MAXNPS)
C
      IF (NPPAX.EQ.1082) THEN
          CALL FILL_GRD1082(DIRVEC)
      ELSE
          CALL DVFILL(NPPA,DIRVEC)
      END IF
C
      DO 100 I=1,107
      CORE(I) = CCORE(I)
100   CONTINUE
      RETURN
      END
