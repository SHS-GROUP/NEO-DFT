     *       ' GRID=',2I4,/
     *       '   COUPV=',F20.10,' DIPOLE=',3F14.8)
 8130 FORMAT('MODE=',3I4,/
     *       ' GRID=',3I4,/
     *       '   TRIPV=',F20.10,' DIPOLE=',3F14.8)
C
C                 WRITE IN RESTART FOR PETYP=QFF
 8210 FORMAT('MODE=',I4,/
     *       ' GRID=',I4,/
     *       '   DE1  =',F20.10,' DDPL1 =',3F14.8)
