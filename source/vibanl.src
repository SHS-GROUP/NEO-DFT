 8035 FORMAT(1x,I5,F12.3,4x,A4,1x,2F12.6)
 8040 FORMAT(1P,3E17.9)
 8050 FORMAT('----- END OF NORMAL MODES FOR -MOLPLT- PROGRAM -----')
 8060 FORMAT('----- HESSIAN MATRIX AFTER PROJECTION -----')
C                            PRINT FORMATS
