C
 9580 FORMAT(6X,'E(MP2)        =',F20.10/
     *       6X,'E(MP2-LAST)   =',F20.10/
     *       6X,'DELTA-E       =',F20.10/)
 9590 FORMAT(1X,'WFNMP2: COSMO PARAMS MPPROP=',I3,' MP2ITER=',I3,
