!PASSED IN VARIABLES
      INTEGER IEXSPA,IPER,IA,JA,NVAL,NA,NB,NX,ITGB,NSYM,NCI,ISPA,
     $     ISYL,IP1,NPTST

!PASSED IN ARRAYS
      INTEGER IBELAC2(*),ISYMB(*),IACON1(*),IACON2(*),IBCON2(*),
