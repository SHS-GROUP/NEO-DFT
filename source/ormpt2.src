      DOUBLE PRECISION COEFF(*),CI(NCI,NPTST),SI2(*),EINT(*)

!LOCAL VARIABLES
      INTEGER IELE2,IA2,JA2,IST2,IEN2,IGAP2,KKJ2,ISWAP,IPER2,IPERT,IGA,
