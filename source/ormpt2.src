      IMPLICIT NONE
      LOGICAL FDIRCT,QCORR
      INTEGER NSPACE,MSTA(51),MNUM(51),MINI(51),MAXI(51), 
     $     IAMI(51),IAMA(51),IBMI(51),IBMA(51),IDIM(51), 
     $     LBST(51),NREF0
      DOUBLE PRECISION C0SQ
      COMMON /FCCWFN/ NSPACE,MSTA,MNUM,MINI,MAXI, 
