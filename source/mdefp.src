C     x = sqrt(-2*ln(u1)) * cos(2*pi*u2)
C     gives normally distrib x on (-inf,+inf)
C
      IF(FIRST) ISEED=7654321
      FIRST=.FALSE.
C
      PI=4.0D+00*ATAN(1.0D+00)
      NTEST=NFRG*3
      XMEAN=0.0D+00
C
      DO I=1,NTEST
        CALL RANBS(ISEED,U1)
        CALL RANBS(ISEED,U2)
        SET(I) = SQRT(-2.0D+00*LOG(U1)) * COS(2.0D+00*PI*U2)
