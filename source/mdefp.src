C     sum over m*v**2 has units of kcal/mole
C
C
C      energy=0.0d+00
      J=0
      DO I=1,NFRG
         SIGMAX=SQRT(BOLTZK*TEMP/AMOM(1,ISET(I)))
         SIGMAY=SQRT(BOLTZK*TEMP/AMOM(2,ISET(I)))
         SIGMAZ=SQRT(BOLTZK*TEMP/AMOM(3,ISET(I)))
         WX(I)=SIGMAX*SET(J+1)
         WY(I)=SIGMAY*SET(J+2)
         WZ(I)=SIGMAZ*SET(J+3)
         J=J+3
