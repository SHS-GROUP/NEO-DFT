      COMMON /PAR   / ME,MASTER,NPROC,IBTYP,IPTIM,GOPARR,DSKWRK,MASWRK
C
C     HUI LI, JAN 2011
C
C     ----- PRINT ENERGY GRADIENT VECTOR FOR MM ATOMS -----
C
      IF (MASWRK.AND.NFFAT.GT.0) THEN
         DO IFFAT=1,NFFAT
            WRITE(IW,9000)ATMNAM(IFFAT),
