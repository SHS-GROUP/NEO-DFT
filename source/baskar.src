      IGAUSS = 1
      IG = 21
      GO TO 700
  643 ITYP = 3
      IGAUSS = 1
      IG = 22
      GO TO 700
C
C     ----- AL TO AR ----
C
  900 GO TO (180,180,180,180,180,180,180,500,520,
     1 540,560,580,910,911,920,930,940,941,942,943),IPASS
  910 ITYP = 2
      IGAUSS = 5
      IG = 14
      GO TO 700
  911 ITYP = 2
      IGAUSS = 1
      IG = 19
      GO TO 700
  920 ITYP = 2
      IGAUSS = 1
      IG = 20
      GO TO 700
  930 ITYP = 2
      IGAUSS = 1
      IG = 21
      GO TO 700
