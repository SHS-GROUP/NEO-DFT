      IGAUSS = 1
      IG = 19
      GO TO 700
  480 IF (NUCZ .GT. 12) GO TO 900
C
C     ----- NA & MG -----
C
      GO TO (180,180,180,180,180,180,180,180,180,180,500,
     1 520,540,560,580,620,640,641,642,643),IPASS
  500 ITYP = 1
      IGAUSS = 7
      IG = 0
      GO TO 700
  520 ITYP = 1
      IGAUSS = 3
      IG = 7
      GO TO 700
