  361 IF (NUCZ .GT. 10) GO TO 480
C
C     ----- B TO NE -----
C
      GO TO (180,180,180,180,180,180,180,180,180,380,
     1 400,420,430,435,440,460,470,471,472,473),IPASS
  380 ITYP = 1
      IGAUSS = 6
      IG = 0
      GO TO 700
