  313 ITYP = 2
      IGAUSS = 1
      IG = 13
      GO TO 700
  360 IF (NUCZ .GT. 4) GO TO 361
C
C     -----  BE -----
C
      GO TO (180,180,180,180,180,180,180,180,180,180,180,
     1 180,261,281,301,306,315,316,317,318),IPASS
  261 ITYP = 1
      IGAUSS = 6
      IG = 0
      GO TO 700
