      IG = 6
      GO TO 700
  233 ITYP = 3
      IGAUSS = 1
      IG = 7
      GO TO 700
  240 IF (NUCZ .GT. 3) GO TO 360
C
C     ----- LI -----
C
      GO TO (180,180,180,180,180,180,180,180,180,180,
     1 180,180,260,280,300,305,310,311,312,313),IPASS
  260 ITYP = 1
      IGAUSS = 6
      IG = 0
      GO TO 700
