C 19 DEC 12 - JI  - Add h/i functions, i.e., use true PCn basis sets
C 20 NOV 08 - MWS - TRAP ON ZERO OR NEGATIVE CHARGE
C 23 MAY 08 - MWS - SUPPRESS SOME DEBUGGING OUTPUT
C 20 AUG 07 - FRJ - PROVIDE ALL PC-N BASIS SETS UP TO ARGON
C 19 SEP 05 - MWS - ADD TRUE NUCLEAR CHARGE ARRAY TO INFOA COMMON
C  5 JUL 05 - MWS - SELECT NEW ATOM,BASIS,EFP,PCM,DAF DIMENSIONS
C 13 FEB 05 - MWS - PAD COMMON BLOCK NSHEL
C  4 FEB 05 - FRJ - ADD PC-N BASIS SETS FOR H,C,N,O,F,SI,P,S,CL.
C
C*MODULE BASPCN  *DECK PCNBAS
C
C> @brief : Sets up the PCn and APCn basis sets.
C>
C> @date  : December 21, 2012 - Joe Ivanic
C>          Include g(H,He),h,i functions, i.e., make FULL basis sets.
C>          Please note that most of the work is done in this (PCNBAS)
C>          and PCNSHL routines, and include normalizations.
C>          All (other) lower subroutines in this module assign explicit
C>          data such as types/numbers/contractions of functions and
C>          exponent values.
C>          These have been modified to include FULL PCn basis sets:
C>          PCN001, PCN002, PCN003, APCN01, APCN02, APCN03.
C
      SUBROUTINE PCNBAS(NUCZ,CSINP,CPINP,CDINP,CFINP,CGINP,chinp,ciinp,
     *       IERR1,IERR2,INTYP,NANGM,NBFS,MINF,MAXF,LOC,NGAUSS,NS,NPOL)
C
      IMPLICIT DOUBLE PRECISION (A-H,O-Z)
C
      LOGICAL GOPARR,DSKWRK,MASWRK
      LOGICAL LDIFF
C
      DIMENSION CSINP(*),CPINP(*),CDINP(*),CFINP(*),CGINP(*),
     *          chinp(*),ciinp(*),
     *          INTYP(*),NANGM(*),NBFS(*),MINF(*),MAXF(*),NS(*)
      DIMENSION EEX(90),CCS(90),CCP(90),CCD(90),CCF(90),CCG(90),
     *          cch(90),cci(90),
     *          ITYPES(50),NGBFS(50)
C
      PARAMETER (MXSH=5000, MXGTOT=20000, MXATM=2000)
C
      COMMON /INFOA / NAT,ICH,MUL,NUM,NQMT,NE,NA,NB,
     *                ZAN(MXATM),C(3,MXATM),IAN(MXATM)
      COMMON /NSHEL / EX(MXGTOT),CS(MXGTOT),CP(MXGTOT),CD(MXGTOT),
     *                CF(MXGTOT),CG(MXGTOT),CH(MXGTOT),CI(MXGTOT),
     *                KSTART(MXSH),KATOM(MXSH),KTYPE(MXSH),KNG(MXSH),
     *                KLOC(MXSH),KMIN(MXSH),KMAX(MXSH),NSHELL
      COMMON /IOFILE/ IR,IW,IP,IS,IPK,IDAF,NAV,IODA(950)
      COMMON /OUTPUT/ NPRINT,ITOL,ICUT,NORMF,NORMP,NOPK
      COMMON /PAR   / ME,MASTER,NPROC,IBTYP,IPTIM,GOPARR,DSKWRK,MASWRK
C
      PARAMETER (ZERO=0.0D+00, ONE=1.0D+00, PT5=0.5D+00, PT75=0.75D+00)
      PARAMETER (PI32=5.56832799683170D+00, TOL=1.0D-10)
      PARAMETER (PT1875=1.875D+00, PT65625=6.5625D+00,
     *           PT2953=29.53125D+00, PT1624=162.421875D+00)
C
C     ----- SET UP THE PC-N BASIS SETS -----
C
      DO 100 I = 1,90
         EEX(I) = ZERO
         CCS(I) = ZERO
         CCP(I) = ZERO
         CCD(I) = ZERO
         CCF(I) = ZERO
         CCG(I) = ZERO
         CCH(I) = ZERO
         CCI(I) = ZERO
  100 CONTINUE
      DO 101 I = 1,50
         ITYPES(I) = 0
         NGBFS(I)  = 0
  101 CONTINUE
      LDIFF = .FALSE.
      NTMP = NPOL
      IF (NTMP.GE.10) THEN
         LDIFF = .TRUE.
         NTMP = NTMP - 10
      ENDIF
      ILAST = 0
C
      IF(NUCZ.LE.0) THEN
         IF(MASWRK) WRITE(IW,9020) 'PC-TYPE',NUCZ
         CALL ABRT
      END IF
 9020 FORMAT(1X,'REQUESTED BUILT-IN ',A,
     *          ' BASIS FOR ATOM WITH ILLEGAL CHARGE=',I4)
C
C     ----- HYDROGEN TO HELIUM -----
C
      IF (NUCZ .GT. 2) GO TO 220
      CALL PCN001(EEX,CCS,CCP,CCD,CCF,ccg,cch,cci,NUCZ,NTMP,ILAST)
      IF (LDIFF) 
     *   CALL APCN01(EEX,CCS,CCP,CCD,CCF,ccg,cch,cci,NUCZ,NTMP,ILAST)
      GO TO 300
C
C     ----- LITHIUM TO NEON -----
C
  220 CONTINUE
      IF (NUCZ.GT.10) GO TO 230
      CALL PCN002(EEX,CCS,CCP,CCD,CCF,CCG,cch,cci,NUCZ,NTMP,ILAST)
      IF (LDIFF) 
     *    CALL APCN02(EEX,CCS,CCP,CCD,CCF,CCG,cch,cci,NUCZ,NTMP,ILAST)
      GO TO 300
C
C     ----- SODIUM TO ARGON -----
C
  230 CONTINUE
      IF(NUCZ.GT.18) GO TO 250
      CALL PCN003(EEX,CCS,CCP,CCD,CCF,CCG,cch,cci,NUCZ,NTMP,ILAST)
      IF (LDIFF) 
     *    CALL APCN03(EEX,CCS,CCP,CCD,CCF,CCG,cch,cci,NUCZ,NTMP,ILAST)
      GO TO 300
C
  250 CONTINUE
      IF (MASWRK) WRITE(6,*)'PC-N BASIS SETS EXIST ONLY UP TO AR.'
      CALL BERROR(3)
      STOP
C
C     ----- NOW SET UP EACH SHELL -----
C
  300 CONTINUE
      CALL PCNSHL(NUCZ,MXPASS,ITYPES,NGBFS,NTMP,LDIFF)
C
      IGX = 0
      IPASS = 0
  310 CONTINUE
      IPASS = IPASS + 1
      IF(IPASS.GT.MXPASS) RETURN
C                         ******
      IGAUSS = NGBFS(IPASS)
      ITYP = ITYPES(IPASS)
C
      NSHELL = NSHELL+1
      IF(NSHELL.GT.MXSH) THEN
         IERR1=1
         RETURN
      END IF
      NS(NAT) = NS(NAT)+1
      KMIN(NSHELL) = MINF(ITYP)
      KMAX(NSHELL) = MAXF(ITYP)
      KSTART(NSHELL) = NGAUSS+1
      KATOM(NSHELL) = NAT
      KTYPE(NSHELL) = NANGM(ITYP)
      INTYP(NSHELL) = ITYP
      KNG(NSHELL) = IGAUSS
      KLOC(NSHELL) = LOC+1
      NGAUSS = NGAUSS+IGAUSS
      IF(NGAUSS.GT.MXGTOT) THEN
         IERR2=1
         RETURN
      END IF
      LOC = LOC+NBFS(ITYP)
      K1 = KSTART(NSHELL)
      K2 = K1+KNG(NSHELL)-1
      DO 720 I = 1,IGAUSS
         K = K1+I-1
         EX(K)    = EEX(IGX+I)
         CSINP(K) = CCS(IGX+I)
         CPINP(K) = CCP(IGX+I)
         CDINP(K) = CCD(IGX+I)
         CFINP(K) = CCF(IGX+I)
         CGINP(K) = CCG(IGX+I)
         CHINP(K) = CCH(IGX+I)
         CIINP(K) = CCI(IGX+I)
         CS(K) = CSINP(K)
         CP(K) = CPINP(K)
         CD(K) = CDINP(K)
         CF(K) = CFINP(K)
         CG(K) = CGINP(K)
         CH(K) = CHINP(K)
         CI(K) = CIINP(K)
  720 CONTINUE
      IGX = IGX + IGAUSS
C
C     ----- ALWAYS UNNORMALIZE PRIMITIVES -----
C
      DO 740 K = K1,K2
         EE = EX(K)+EX(K)
         FACS = PI32/(EE*SQRT(EE))
         FACP = PT5*FACS/EE
         FACD = PT75*FACS/(EE*EE)
         FACF = PT1875*FACS/(EE*EE*EE)
         FACG = PT65625*FACS/(EE*EE*EE*EE)
         FACH = PT2953*FACS/(EE*EE*EE*EE*EE)
         FACI = PT1624*FACS/(EE*EE*EE*EE*EE*EE)
         CS(K) = CS(K)/SQRT(FACS)
         CP(K) = CP(K)/SQRT(FACP)
         CD(K) = CD(K)/SQRT(FACD)
         CF(K) = CF(K)/SQRT(FACF)
         CG(K) = CG(K)/SQRT(FACG)
         CH(K) = CH(K)/SQRT(FACH)
         CI(K) = CI(K)/SQRT(FACI)
  740 CONTINUE
C
C     ----- IF(NORMF.EQ.0) NORMALIZE BASIS FUNCTIONS. -----
C
      IF (NORMF .EQ. 1) GO TO 820
      FACS = ZERO
      FACP = ZERO
      FACD = ZERO
      FACF = ZERO
      FACG = ZERO
      FACH = ZERO
      FACI = ZERO
      DO 780 IG = K1,K2
         DO 770 JG = K1,IG
            EE = EX(IG)+EX(JG)
            FAC = EE*SQRT(EE)
            DUMS = CS(IG)*CS(JG)/FAC
            DUMP = PT5*CP(IG)*CP(JG)/(EE*FAC)
            DUMD = PT75*CD(IG)*CD(JG)/(EE*EE*FAC)
            DUMF = PT1875*CF(IG)*CF(JG)/(EE*EE*EE*FAC)
            DUMG = PT65625*CG(IG)*CG(JG)/(EE*EE*EE*EE*FAC)
            DUMH = PT2953*CH(IG)*CH(JG)/(EE*EE*EE*EE*EE*FAC)
            DUMI = PT1624*CI(IG)*CI(JG)/(EE*EE*EE*EE*EE*EE*FAC)
            IF (IG .NE. JG) THEN
               DUMS = DUMS+DUMS
               DUMP = DUMP+DUMP
               DUMD = DUMD+DUMD
               DUMF = DUMF+DUMF
               DUMG = DUMG+DUMG
               DUMH = DUMH+DUMH
               DUMI = DUMI+DUMI
            END IF
            FACS = FACS+DUMS
            FACP = FACP+DUMP
            FACD = FACD+DUMD
            FACF = FACF+DUMF
            FACG = FACG+DUMG
            FACH = FACH+DUMH
            FACI = FACI+DUMI
  770    CONTINUE
  780 CONTINUE
      IF (FACS .GT. TOL) FACS = ONE/SQRT(FACS*PI32)
      IF (FACP .GT. TOL) FACP = ONE/SQRT(FACP*PI32)
      IF (FACD .GT. TOL) FACD = ONE/SQRT(FACD*PI32)
      IF (FACF .GT. TOL) FACF = ONE/SQRT(FACF*PI32)
      IF (FACG .GT. TOL) FACG = ONE/SQRT(FACG*PI32)
      IF (FACH .GT. TOL) FACH = ONE/SQRT(FACH*PI32)
      IF (FACI .GT. TOL) FACI = ONE/SQRT(FACI*PI32)
      DO 800 IG = K1,K2
         CS(IG) = CS(IG) * FACS
         CP(IG) = CP(IG) * FACP
         CD(IG) = CD(IG) * FACD
         CF(IG) = CF(IG) * FACF
         CG(IG) = CG(IG) * FACG
         CH(IG) = CH(IG) * FACH
         CI(IG) = CI(IG) * FACI
         CSINP(IG) = CSINP(IG) * FACS
         CPINP(IG) = CPINP(IG) * FACP
         CDINP(IG) = CDINP(IG) * FACD
         CFINP(IG) = CFINP(IG) * FACF
         CGINP(IG) = CGINP(IG) * FACG
         CHINP(IG) = CHINP(IG) * FACH
         CIINP(IG) = CIINP(IG) * FACI
  800 CONTINUE
C
C        AND BRANCH BACK FOR THE NEXT SHELL
C
  820 CONTINUE
      GO TO 310
      END
C*MODULE BASPCN  *DECK PCNSHL
C
C> @brief : Sets up the PCn and APCn basis sets.
C>
C> @date  : December 21, 2012 - Joe Ivanic
C>          Include g(H,He),h,i functions, i.e., make FULL basis sets.
C>          Please note that most of the work is done in this (PCNSHL)
C>          and PCNBAS routines, and include normalizations.
C>          All (other) lower subroutines in this module assign explicit
C>          data such as types/numbers/contractions of functions and
C>          exponent values.
C>          These have been modified to include FULL PCn basis sets:
C>          PCN001, PCN002, PCN003, APCN01, APCN02, APCN03.
C
      SUBROUTINE PCNSHL(NUCZ,MXPASS,ITYPES,NGBFS,NTMP,LDIFF)
      IMPLICIT DOUBLE PRECISION (A-H,O-Z)
      DIMENSION ITYPES(50),NGBFS(50)
      DIMENSION NPASS(5,5)
      DIMENSION ITYPS(32,5,5),NGS(32,5,5)
      LOGICAL LDIFF
C
C     NPASS: NUMBER OF CONTRACTED SHELLS FOR: H ; LI-BE ; B-NE ;
C                                             NA-MG ; AL-AR
C
      DATA NPASS/2,3,6,12,19,   4,5,7,12,18,    5,6,10,18,27,
     *           6,6,9,13,18,   7,8,12,18,25/
C
C     ITYPS DEFINES THE TYPE OF SHELL, 1=S,2=P,3=D,4=F,5=G,6=H,7=I
C
C H,He
      DATA ITYPS/2*1,30*0,   2*1,1*2,29*0,   3*1,2*2,1*3,26*0,
     *           5*1,4*2,2*3,1*4,20*0,  7*1,6*2,3*3,2*4,1*5,13*0,
C Li,Be
     *
     *           3*1,2,28*0,   3*1,2*2,27*0,   4*1,2*2,1*3,25*0,
     *           6*1,3*2,2*3,1*4,20*0,  8*1,4*2,3*3,2*4,1*5,14*0,
C B-Ne
     *
     *           3*1,2*2,27*0,   3*1,2*2,1*3,26*0,
     *           4*1,3*2,2*3,1*4,22*0,
     *           6*1,5*2,4*3,2*4,1*5,14*0,  8*1,7*2,6*3,3*4,2*5,1*6,5*0,
C Na,Mg
     *
     *           4*1,2*2,26*0,   4*1,2*2,26*0,
     *           5*1,3*2,1*3,23*0,
     *           6*1,4*2,2*3,1*4,19*0,  7*1,5*2,3*3,2*4,1*5,14*0,
C Al-Ar
     *
     *           4*1,3*2,25*0,   4*1,3*2,1*3,24*0,
     *           5*1,4*2,2*3,1*4,20*0,
     *           6*1,5*2,4*3,2*4,1*5,14*0,  7*1,6*2,6*3,3*4,2*5,1*6,7*0/
C
C       NGS TELLS THE NUMBER OF GAUSSIANS IN EACH SHELL
C
C H,He
      DATA NGS/  2,1,30*0,   3,1,1,29*0,   4,5*1,26*0,
     *           5,11*1,20*0,  5,18*1,13*0,
C Li,Be
     *
     *           2*3,2*1,28*0,   2*5,1,2,1,27*0,   2*7,2*1,3,2*1,25*0,
     *           2*9,4*1,4,5*1,20*0,  2*12,6*1,5,9*1,14*0,
C B-Ne
     *
     *           2*3,1,2,1,27*0,   2*5,1,3,2*1,26*0,
     *           2*7,2*1,4,5*1,22*0,
     *           9,8,4*1,5,11*1,14*0,  11,9,6*1,5,18*1,5*0,
C Na,Mg
     *
     *           3*5,1,4,1,26*0,   3*8,1,6,1,26*0,
     *           3*9,2*1,7,3*1,23*0,
     *           12,2*10,3*1,9,6*1,19*0,
     *           15,11,11,4*1,11,10*1,14*0,
C Al-Ar
     *
     *           3*5,1,2*4,1,25*0,   3*8,1,2*6,2*1,24*0,
     *           3*9,2*1,2*7,5*1,20*0,
     *           12,2*10,3*1,2*9,10*1,14*0,
     *           15,2*12,4*1,2*11,16*1,7*0/
C
      NTMP = NTMP + 1
CFRJ  NROW NOW INDICATES BOTH ROW AND S OR SP BLOCK ELEMENTS
      NROW = 99
      IF (NUCZ.LE.18) NROW = 5
      IF (NUCZ.LE.12) NROW = 4
      IF (NUCZ.LE.10) NROW = 3
      IF (NUCZ.LE.4)  NROW = 2
      IF (NUCZ.LE.2)  NROW = 1
      MXPASS = NPASS(NTMP,NROW)
      DO 100 I=1,MXPASS
         ITYPES(I) = ITYPS(I,NTMP,NROW)
         NGBFS(I)  = NGS(I,NTMP,NROW)
C        A FEW UGLY HACKS FOR ADJUSTING NGBFS ACCORDING TO NUCZ.
C        THE DAVIDSON PURIFICATION TO PARTLY SEGMENT THE GENERAL
C        CONTRACTION MEANS THAT THE NUMBER OF FUNCTIONS IS NOT
C        NECESSARILY THE SAME FOR ALL ELEMENTS IN A GIVEN ROW
         IF (NUCZ.EQ.5  .AND. NTMP.EQ.4 .AND. I.EQ.2) NGBFS(I) = 9
         IF (NUCZ.EQ.6  .AND. NTMP.EQ.4 .AND. I.EQ.2) NGBFS(I) = 9
         IF (NUCZ.EQ.6  .AND. NTMP.EQ.5 .AND. I.EQ.2) NGBFS(I) = 10
         IF (NUCZ.EQ.11 .AND. NTMP.EQ.3 .AND. I.EQ.2) NGBFS(I) = 8
         IF (NUCZ.EQ.11 .AND. NTMP.EQ.5 .AND. I.EQ.3) NGBFS(I) = 13
         IF (NUCZ.EQ.13 .AND. NTMP.EQ.5 .AND. I.EQ.2) NGBFS(I) = 11
         IF (NUCZ.EQ.13 .AND. NTMP.EQ.5 .AND. I.EQ.3) NGBFS(I) = 13
         IF (NUCZ.EQ.13 .AND. NTMP.EQ.5 .AND. I.EQ.9) NGBFS(I) = 10
  100 CONTINUE
C     POSSIBLY ADJUST FOR DIFFUSE FUNCTIONS
C--   WRITE(*,*)'LDIFF, NTMP, NROW',LDIFF,NTMP,NROW
      IF (LDIFF) THEN
         NEND = NTMP
C--   WRITE(*,*)'NEND',NEND
         IF (NROW.EQ.3 .OR. NROW.EQ.5) NEND = NEND + 1
C--   WRITE(*,*)'NEND',NEND
         IF (NROW.EQ.2 .AND. NTMP.EQ.1) NEND = NEND + 1
C--   WRITE(*,*)'NEND',NEND
         IF (NROW.EQ.4 .AND. NTMP.EQ.1) NEND = NEND + 1
C--   WRITE(*,*)'NEND',NEND
C--   WRITE(*,*)'NEND',NEND
         DO I = 1,NEND
            ITYPES(MXPASS+1) = I
            NGBFS(MXPASS+1)  = 1
            MXPASS = MXPASS + 1
         ENDDO
      ENDIF
      RETURN
      END
C*MODULE BASPCN  *DECK PCN001
      SUBROUTINE PCN001(E,S,P,D,F,g,h,ai,N,NPOL,ILAST)
      IMPLICIT DOUBLE PRECISION (A-H,O-Z)
      DIMENSION E(90),S(90),P(90),D(90),F(90)
      dimension g(90),h(90),ai(90)
C
      GO TO (100,120),N
C
C     ----- HYDROGEN -----
C
  100 CONTINUE
      IF (NPOL.EQ.0) THEN
         E(1) = 0.4344800D+01
         S(1) = 0.7929900D-01
         E(2) = 0.6604900D+00
         S(2) = 0.4242200D+00
         E(3) = 0.1366900D+00
         S(3) = 0.1000000D+01
         ILAST = 3
      ELSE IF (NPOL.EQ.1) THEN
         E(1) = 0.1225200D+02
         S(1) = 0.2282200D-01
         E(2) = 0.1868700D+01
         S(2) = 0.1556400D+00
         E(3) = 0.4182100D+00
         S(3) = 0.4889800D+00
         E(4) = 0.1061000D+00
         S(4) = 0.1000000D+01
         E(5) = 0.1000000D+01
         P(5) = 0.1000000D+01
         ILAST = 5
      ELSE IF (NPOL.EQ.2) THEN
         E(1) = 0.7542300D+02
         S(1) = 0.2406500D-02
         E(2) = 0.1135000D+02
         S(2) = 0.1848700D-01
         E(3) = 0.2599300D+01
         S(3) = 0.8974200D-01
         E(4) = 0.7351300D+00
         S(4) = 0.2811100D+00
         E(5) = 0.2316700D+00
         S(5) = 0.1000000D+01
         E(6) = 0.7414700D-01
         S(6) = 0.1000000D+01
         E(7) = 0.1600000D+01
         P(7) = 0.1000000D+01
         E(8) = 0.4500000D+00
         P(8) = 0.1000000D+01
         E(9) = 0.1250000D+01
         D(9) = 0.1000000D+01
         ILAST = 9
      ELSE IF (NPOL.EQ.3) THEN
         E(1) = 0.7746900D+03
         S(1) = 0.1316300D-02
         E(2) = 0.1161600D+03
         S(2) = 0.1022600D-01
         E(3) = 0.2644900D+02
         S(3) = 0.5362900D-01
         E(4) = 0.7501500D+01
         S(4) = 0.2228500D+00
         E(5) = 0.2456800D+01
         S(5) = 0.7537400D+00
         E(6) = 0.8877400D+00
         S(6) = 0.1000000D+01
         E(7) = 0.3408500D+00
         S(7) = 0.1000000D+01
         E(8) = 0.1347700D+00
         S(8) = 0.1000000D+01
         E(9) = 0.5226300D-01
         S(9) = 0.1000000D+01
         E(10) = 0.6500000D+01
         P(10) = 0.1000000D+01
         E(11) = 0.1550000D+01
         P(11) = 0.1000000D+01
         E(12) = 0.5500000D+00
         P(12) = 0.1000000D+01
         E(13) = 0.2000000D+00
         P(13) = 0.1000000D+01
         E(14) = 0.1950000D+01
         D(14) = 0.1000000D+01
         E(15) = 0.6500000D+00
         D(15) = 0.1000000D+01
         E(16) = 0.2200000D+01
         F(16) = 0.1000000D+01
         ILAST = 16
      ELSE IF (NPOL.EQ.4) THEN
         E(1) = 0.3079700D+04
         S(1) = 0.2347300D-03
         E(2) = 0.4615200D+03
         S(2) = 0.1824500D-02
         E(3) = 0.1050600D+03
         S(3) = 0.9593300D-02
         E(4) = 0.2976400D+02
         S(4) = 0.4046100D-01
         E(5) = 0.9718300D+01
         S(5) = 0.1460700D+00
         E(6) = 0.3517900D+01
         S(6) = 0.1000000D+01
         E(7) = 0.1377000D+01
         S(7) = 0.1000000D+01
         E(8) = 0.5683100D+00
         S(8) = 0.1000000D+01
         E(9) = 0.2434100D+00
         S(9) = 0.1000000D+01
         E(10) = 0.1050500D+00
         S(10) = 0.1000000D+01
         E(11) = 0.4419800D-01
         S(11) = 0.1000000D+01
         E(12) = 0.3000000D+02
