! http://staff.aist.go.jp/d.g.fedorov/fmo/main.html
!
 $contrl scftyp=rhf runtyp=gradient ispher=1 nprint=-5 $end
 $dft    dfttyp=B3LYP swoff=1e-3 
 NRAD=96 NTHE=12 NPHI=24 NRAD0=96 NTHE0=12 NPHI0=24 $end
 $system mwords=5 $end
----$gddi   ngroup=1 parout=.t. BALTYP=NXTVAL $end
--- $intgrl nintic=-90000000 $end
 $basis  gbasis=n31 ngauss=6 ndfunc=1 $end
 $fmo
        nfrag=16 nbody=2 nacut=3
        respap=0 resppc=0 resdim=0 ritrim(1)=0,0,0 modesp=0 modgrd=16
 $end
 $fmoprp coroff=10e-3 nprint=9 modorb=1 $end
 $data
Water16 modeled by HyperChem and optimized by amber94 force feld
C1
h 1
o 8
 $end
